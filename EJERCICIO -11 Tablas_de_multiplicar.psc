	Algoritmo Tablas_de_multiplicar
	//Definir variables 
	Definir Num Como Entero
	Definir nsn como Entero
    Escribir "Ingrese un Número del 1 - 12: ";
    Leer nsn;
    Para num <-1 Hasta 12 Con Paso 1  Hacer
        Escribir nsn, " X ", num, " = ", (nsn * num);
    FinPara	

FinAlgoritmo
